    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}'"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "querystringify": "^2.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.28.0",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0"
